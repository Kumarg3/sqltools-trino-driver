{
  "name": "sqltools-trino-driver",
  "displayName": "Trino Driver",
  "description": "Trino Driver for SQLTools",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.42.0"
  },
  "publisher": "regadas",
  "license": "Apache",
  "repository": {},
  "bugs": {},
  "scripts": {
    "clean": "rimraf -rf out *.vsix",
    "vscode:prepublish": "npm run esbuild:ext -- --minify && npm run esbuild:ls -- --minify && npm run copy",
    "copy": "cp package.json out/ls",
    "esbuild:ext": "esbuild ./src/extension.ts --bundle --external:vscode --platform=node --tsconfig=./tsconfig.json --log-level=info --color=true --format=cjs --outfile=./out/extension.js",
    "esbuild:ls": "esbuild ./src/ls/plugin.ts --bundle --external:vscode --platform=node --tsconfig=./tsconfig.json --log-level=info --color=true --format=cjs --outfile=./out/ls/plugin.js",
    "esbuild:p": "esbuild package.json --bundle --external:vscode --platform=node --tsconfig=./tsconfig.json --log-level=info --color=true --format=cjs --outfile=./out/ls/package.json",
    "esbuild": "npm run esbuild:ext -- --sourcemap && npm run esbuild:ls -- --sourcemap",
    "test-compile": "tsc -p ./",
    "package": "vsce package --allow-star-activation",
    "watch": "tsc -watch -p ./"
  },
  "keywords": [
    "Trino",
    "Trinodb",
    "trino",
    "trino-driver",
    "trinodb",
    "sqltools-driver"
  ],
  "categories": [
    "Programming Languages",
    "Snippets",
    "Formatters",
    "Other"
  ],
  "extensionDependencies": [
    "mtxr.sqltools"
  ],
  "activationEvents": [
    "*",
    "onLanguage:sql",
    "onCommand:sqltools.*"
  ],
  "main": "./out/extension.js",
  "dependencies": {
    "@sqltools/base-driver": "latest",
    "@sqltools/types": "latest",
    "presto-client": "^0.12.2",
    "uuid": "^7.0.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.11",
    "@types/node": "^17.0.25",
    "@types/uuid": "^8.3.4",
    "@types/vscode": "^1.42.0",
    "esbuild": "^0.14.38",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.3",
    "vsce": "^2.7.0"
  }
}
